class Solution {
public:
    int evalRPN(vector<string>& s) {
        stack<int> st;

        for (auto &c : s) {
            if (c == "+") {
                int a = st.top(); 
                st.pop(); 

                int b = st.top(); 
                st.pop(); 

                st.push(b + a);
            } else if (c == "-") {
                int a = st.top(); 
                st.pop(); 

                int b = st.top(); 
                st.pop(); 

                st.push(b - a);
            } else if (c == "*") {
                int a = st.top(); 
                st.pop(); 

                int b = st.top(); 
                st.pop(); 

                st.push(b * a);
            } else if (c == "/") {
                int a = st.top(); 
                st.pop(); 

                int b = st.top(); 
                st.pop(); 

                st.push(b / a);
            } else {
                st.push(stoi(c));
            }
        }

        return st.top();
    }
};
